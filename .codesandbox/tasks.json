{
  "setupTasks": [
    {
      "command": "yarn install",
      "name": "Installing Dependencies"
    }
  ],
  "tasks": {
    "start": {
      "name": "start",
      "command": "yarn build && yarn preview --host",
      "runAtStart": true,
      "preview": {
        "port": 4173
      }
    },
    "dev": {
      "name": "dev",
      "command": "yarn dev --host",
      "preview": {
        "port": 5173
      }
    },
    "build": {
      "name": "build",
      "command": "yarn build"
    },
    "preview": {
      "name": "preview",
      "command": "yarn preview --host"
    }
  }
}